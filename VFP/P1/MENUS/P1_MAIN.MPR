	MESSAGE "Répète la dernière commande ou action"
DEFINE BAR _med_sp100 OF _medit PROMPT "\-"
DEFINE BAR _med_cut OF _medit PROMPT "\<Couper" ;
	KEY CTRL+X, "Ctrl+X" ;
	MESSAGE "Supprime la sélection et la place dans le Presse-papiers"
DEFINE BAR _med_copy OF _medit PROMPT "Co\<pier" ;
	KEY CTRL+C, "Ctrl+C" ;
	MESSAGE "Copie la sélection dans le Presse-papiers"
DEFINE BAR _med_paste OF _medit PROMPT "C\<oller" ;
	KEY CTRL+V, "Ctrl+V" ;
	MESSAGE "Insère le contenu du Presse-papiers au point d'insertion"
DEFINE BAR _med_clear OF _medit PROMPT "E\<ffacer" ;
	MESSAGE "Supprime la sélection sans la placer dans le Presse-papiers"
DEFINE BAR _med_sp200 OF _medit PROMPT "\-"
DEFINE BAR _med_slcta OF _medit PROMPT "\<Sélectionner tout" ;
	KEY CTRL+A, "Ctrl+A" ;
	MESSAGE "Sélectionne l'ensemble du texte ou des éléments de la fenêtre active"
DEFINE BAR _med_sp300 OF _medit PROMPT "\-"
DEFINE BAR _med_find OF _medit PROMPT "Rec\<hercher..." ;
	KEY CTRL+F, "Ctrl+F" ;
	MESSAGE "Recherche le texte spécifié"
DEFINE BAR _med_finda OF _medit PROMPT "Chercher à \<nouveau" ;
	KEY CTRL+G, "Ctrl+G" ;
	MESSAGE "Répète la dernière recherche"
DEFINE BAR _med_repl OF _medit PROMPT "R\<emplacer..." ;
	KEY CTRL+L, "Ctrl+L" ;
	MESSAGE "Remplace le texte spécifié par du texte différent"

DEFINE POPUP _mtools MARGIN RELATIVE SHADOW COLOR SCHEME 4
DEFINE BAR 1 OF _mtools PROMPT "M\<acros..." ;
	MESSAGE "Crée, supprime ou modifie une macro clavier"
DEFINE BAR 2 OF _mtools PROMPT "Journaux des \<erreurs"
DEFINE BAR 3 OF _mtools PROMPT "\-"
DEFINE BAR 4 OF _mtools PROMPT "\<Options..."
